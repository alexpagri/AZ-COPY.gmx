if(vkfp==0 &amp;&amp; vkfpo==1)vkfrx=1;

if(((keyboard_check(key_up) &amp;&amp; h_key) || vku) &amp;&amp; stuck==0 &amp;&amp; started)
{
physics_apply_impulse(phy_com_x,phy_com_y,forces*cos(phy_rotation*pi/180),forces*sin(phy_rotation*pi/180));
}
if(((keyboard_check(key_left) &amp;&amp; h_key) || vkl) &amp;&amp; stuck==0 &amp;&amp; started)
{
physics_apply_angular_impulse(-ang_forces);
}
if(((keyboard_check(key_down) &amp;&amp; h_key) || vkd) &amp;&amp; stuck==0 &amp;&amp; started)
{
physics_apply_impulse(phy_com_x,phy_com_y,-back_forces*cos(phy_rotation*pi/180),-back_forces*sin(phy_rotation*pi/180));
}
if(((keyboard_check(key_right) &amp;&amp; h_key) || vkr) &amp;&amp; stuck==0 &amp;&amp; started)
{
physics_apply_angular_impulse(ang_forces);
}


if(debug_mode&amp;&amp;keyboard_check_pressed(ord('0')))
{
weapon_current=0;
block_fire=0;
pr5=noone;
}
